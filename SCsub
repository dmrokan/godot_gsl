]

env_godot_gsl = env
env_godot_gsl.Command('', '', 'cd gsl4godot_gsl')
env_godot_gsl.Command('', '', './autogen.sh')
env_godot_gsl.Command('', '', './configure')
env_godot_gsl.Command('', '', 'cd ..')
env_godot_gsl.Append(CXXFLAGS=['-g'])
env_godot_gsl.Append(CPPPATH=scons_paths)
env_godot_gsl.add_source_files(env.modules_sources, scons_sources)