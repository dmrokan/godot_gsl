    , [x for x in Glob('./gsl4godot_gsl/const/*.c')  if not os.path.basename(str(x)).endswith('_source.c') and os.path.basename(str(x)) not in excluded_files and not os.path.basename(str(x)).startswith('demo') and not os.path.basename(str(x)).startswith('c_pass_') and not os.path.basename(str(x)).startswith('hc_pass_') and not os.path.basename(str(x)).startswith('fp-') and not os.path.basename(str(x)).startswith('real_pass_') and not os.path.basename(str(x)).startswith('test_')]
    , [x for x in Glob('./gsl4godot_gsl/const/.deps/*.c')  if not os.path.basename(str(x)).endswith('_source.c') and os.path.basename(str(x)) not in excluded_files and not os.path.basename(str(x)).startswith('demo') and not os.path.basename(str(x)).startswith('c_pass_') and not os.path.basename(str(x)).startswith('hc_pass_') and not os.path.basename(str(x)).startswith('fp-') and not os.path.basename(str(x)).startswith('real_pass_') and not os.path.basename(str(x)).startswith('test_')]
    , [x for x in Glob('./examples/*.c')  if not os.path.basename(str(x)).endswith('_source.c') and os.path.basename(str(x)) not in excluded_files and not os.path.basename(str(x)).startswith('demo') and not os.path.basename(str(x)).startswith('c_pass_') and not os.path.basename(str(x)).startswith('hc_pass_') and not os.path.basename(str(x)).startswith('fp-') and not os.path.basename(str(x)).startswith('real_pass_') and not os.path.basename(str(x)).startswith('test_')]
    , [x for x in Glob('./examples/LorenzAtt/*.c')  if not os.path.basename(str(x)).endswith('_source.c') and os.path.basename(str(x)) not in excluded_files and not os.path.basename(str(x)).startswith('demo') and not os.path.basename(str(x)).startswith('c_pass_') and not os.path.basename(str(x)).startswith('hc_pass_') and not os.path.basename(str(x)).startswith('fp-') and not os.path.basename(str(x)).startswith('real_pass_') and not os.path.basename(str(x)).startswith('test_')]
